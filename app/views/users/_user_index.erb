<h2>Users</h2>
<table class="table">
  <thead>
    <th>image</th>
    <th>name</th>
    <th></th>
    <th></th>
  </thead>
  <tbody>
    <% users.each do |user| %>
      <tr>
        <td>
          <%= image_tag user.get_profile_image(80, 80) %>
        </td>
        <td><%= user.name %></td>
        <td><%= link_to "Show", user_path(user) %></td>
      </tr>
    <% end %>
  </tbody>
</table>